<p id="notice"><%= notice %></p>

<h1>Questions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @questions.each do |question| %>
      <%= link_to test_question_path(test_id: question.test_id, id: question.id), action: 'go' do %>
        <div>
          <h1><%= question.body %></h1>
        </div>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question', new_test_question_path %>
