
<h1 class="mx-5"><%= t('.header') %></h1>

<table class="table p-5">
  <thead>
    <tr>
      <th class="sort-by-title pointer">
        <%= t('.title') %>
        <%= octicon 'arrow-up', class: 'text-success hide' %>
        <%= octicon 'arrow-down', class: 'text-success hide' %>
      </th>
    </tr>
  </thead>

  <tbody>

    <% @no_received_rewards.each do |reward| %>
      <tr>
        <td><%= reward.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
